@{
    ViewBag.Title = "title";
    Layout = "_layout";
}
@model IEnumerable<Web.ViewModels.AbsenceViewModel>

<div class="flex flex-col space-y-5">
    <div class="flex justify-between">
        <span class="font-bold text-xl">
            Verlof aanvragen beoordelen
        </span>
    </div>
    <div class="flex flex-col space-y-3">
        @foreach (var absence in Model)
        {
            @Html.Partial("Partials/_Absence", absence, new ViewDataDictionary(Html.ViewData) { { "Evaluate", true } })
        }
    </div>
</div>